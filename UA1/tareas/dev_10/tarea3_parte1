
package Act3_Parte1;

/**
 * @author José Ramón Crespo Castro
 */
public class Act3_Parte1 {

    public static void main(String[] args) {
        if (args.length < 1){
            System.exit(1);
        }
        else{
            try{
                int prueba = Integer.parseInt(args[0]);
                if(prueba < 0){
                    System.exit(3);
                }               
                else{
                    System.exit(0);
                }                  
            }catch(Exception e) {
                System.exit(2);
            }
        }
}
