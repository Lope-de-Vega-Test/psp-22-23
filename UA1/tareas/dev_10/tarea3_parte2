
package Act3_Parte2;
import java.io.IOException;

/**
 * @author José Ramón Crespo Castro
 */
public class Act3_Parte2 {

    public static void main(String[] args) {
                        
        ProcessBuilder pb = new ProcessBuilder("Prueba", "C:\\Users\\HP\\OneDrive\\Escritorio\\Dam2\\P.Servicios y procesos\\UA1\\Act3_Parte2\\Act3_Parte2.java","Prueba");
		
	try {
            pb.redirectInput(ProcessBuilder.Redirect.INHERIT);
            pb.redirectOutput(ProcessBuilder.Redirect.INHERIT);

            Process p = pb.start();

            try {
		p.waitFor();
                System.out.println("Devuelve: "+p.exitValue());
		} catch (InterruptedException e1) {
				e1.printStackTrace();
		}
         
	} catch (IOException e2) {
			e2.printStackTrace();
	}
    }
}
